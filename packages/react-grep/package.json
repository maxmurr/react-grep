{
  "name": "react-grep",
  "version": "0.3.0",
  "description": "Hold CMD to see React component names + file:line overlaid on any element",
  "keywords": [
    "component",
    "debug",
    "devtools",
    "fiber",
    "inspect",
    "inspector",
    "react",
    "source"
  ],
  "homepage": "https://react-grep.com",
  "bugs": {
    "url": "https://github.com/maxmurr/react-grep/issues"
  },
  "license": "MIT",
  "author": "Max Murray",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxmurr/react-grep.git",
    "directory": "packages/react-grep"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "type": "module",
  "sideEffects": true,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "browser": "dist/index.global.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.18",
    "jsdom": "^28.0.0",
    "tsup": "^8.2.4",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  }
}
